{
  "name": "cvo-technologies/github",
  "type": "cakephp-plugin",
  "description": "GitHub plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Marlin Cremers",
      "email": "m.cremers@cvo-technologies.com"
    }
